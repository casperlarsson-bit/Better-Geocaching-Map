{
	"manifest_version": 3,
	"name": "Remove play map",
	"version": "0.1",
	"description": "Removes/changes play map UI elements.",

	"action": {
		"default_icon": "map.png"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"run_at": "document_idle"
		}
	],

	"permissions": [],
	"host_permissions": ["<all_urls>"]
}
